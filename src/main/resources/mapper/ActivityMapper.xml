<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tencent.wxcloudrun.dao.ActivityMapper">


    <insert id="createActivity" parameterType="Activity">
        INSERT INTO activity (title,startDate,location,price,capacity,imgs,additionalInfo,description,userJoined) values (#{title},#{startDate},#{location},#{price},#{capacity},#{imgs},#{additionalInfoJSON},#{description},#{userJoinedJSON})
    </insert>

</mapper>
